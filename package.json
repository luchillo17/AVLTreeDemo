{
  "name": "avltree-demo",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build --prod --aot && cp src/electron.js src/package.json dist/AVLTreeDemo",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:ci": "jest --runInBand",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "package": "electron-packager dist/AVLTreeDemo AVLTreeDemo --asar --all --overwrite --out package && for i in package/*; do zip -r \"${i%/}.zip\" \"$i\"; done",
    "deploy": "yarn build && yarn package"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^6.0.3",
    "@angular/cdk": "^6.2.0",
    "@angular/common": "^6.0.3",
    "@angular/compiler": "^6.0.3",
    "@angular/core": "^6.0.3",
    "@angular/forms": "^6.0.3",
    "@angular/http": "^6.0.3",
    "@angular/material": "^6.2.0",
    "@angular/platform-browser": "^6.0.3",
    "@angular/platform-browser-dynamic": "^6.0.3",
    "@angular/router": "^6.0.3",
    "core-js": "^2.5.4",
    "d3": "^5.4.0",
    "electron": "^2.0.2",
    "hammerjs": "^2.0.8",
    "lodash": "^4.17.10",
    "rxjs": "^6.2.0",
    "zone.js": "^0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.7.0-beta.1",
    "@angular/cli": "~6.0.7",
    "@angular/compiler-cli": "^6.0.3",
    "@angular/language-service": "^6.0.3",
    "@types/d3": "^5.0.0",
    "@types/jest": "^23.0.0",
    "@types/lodash": "^4.14.109",
    "@types/node": "^10.3.0",
    "codelyzer": "^4.3.0",
    "devtron": "^1.4.0",
    "electron-debug": "^2.0.0",
    "electron-packager": "^12.1.0",
    "jest": "^23.1.0",
    "jest-preset-angular": "^5.2.2",
    "karma": "^2.0.2",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "^2.0.1",
    "prettier": "^1.12.1",
    "protractor": "~5.3.0",
    "ts-node": "^6.1.0",
    "tslint": "^5.10.0",
    "typescript": "<2.8.0"
  },
  "jest": {
    "preset": "jest-preset-angular",
    "setupTestFrameworkScriptFile": "<rootDir>/configs/setupJest.ts"
  }
}
